@charset "utf-8";

body, h1, h2, h3, h4, h5, h6, view, img, p {
  margin: 0;
  padding: 0;
}

html {
  width: 100%;
}

body {
  width: 100%;
}

body, h1, h2, h3, h4, h5, h6, view {
  font-size: 24rpx;
  font-family: 'Microsoft Yahei', Tahoma, Arial, Verdana;
  color: black;
  font-weight: normal;
}

em, i {
  font-style: normal;
}

strong, b {
  font-weight: normal;
}

li {
  list-style: none;
}

image {
  border: 0;
}

a {
  color: #0076ff;
  text-decoration: none;
}
